{
	"name": "Streamable Shortcuts",
	"version": "1.0.1",
	"description": "Streamable Shortcuts",
	"homepage_url": "https://github.com/jaebradley/streamable-shortcuts-extension",
	"manifest_version": 2,
	"minimum_chrome_version": "58",
	"permissions": [
		"*://streamable.com/*",
		"tabs",
		"webNavigation"
	],
	"optional_permissions": [
		"http://*/*",
		"https://*/*"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"content_scripts": [
		{
			"run_at": "document_start",
			"matches": [
				"*://streamable.com/*"
			],
			"js": [
				"content.js"
			],
			"css": [
				"index.css"
			]
		}
	],
	"icons": {
		"16": "16.png",
		"48": "48.png",
		"128": "128.png"
	},
	"web_accessible_resources": [
		"shortcuts.js",
		"help.html",
		"index.css"
  ]
}
